{
  "name": "freevibes",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently -n web,proxy -c blue,green \"npm run dev -w @freevibes/web\" \"npm run dev -w @freevibes/cors-proxy\"",
    "build": "npm run build -w @freevibes/web",
    "preview": "npm run preview -w @freevibes/web",
    "deploy:proxy": "npm run deploy -w @freevibes/cors-proxy"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
